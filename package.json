{
  "name": "elabftw",
  "description": "Free and open source electronic lab notebook for research labs",
  "main": "builder.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "3dmol": "^1.4.0",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@deltablot/chemdoodle-web-mini": "^9.2.1",
    "@deltablot/open-vector-editor-umd": "^16.0.1",
    "@fancyapps/fancybox": "3.5.*",
    "@fortawesome/fontawesome-svg-core": "1.2.*",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fullcalendar/bootstrap": "^5.1.0",
    "@fullcalendar/core": "^5.1.0",
    "@fullcalendar/daygrid": "^5.1.0",
    "@fullcalendar/interaction": "^5.1.0",
    "@fullcalendar/list": "^5.1.0",
    "@fullcalendar/timegrid": "^5.1.0",
    "babel-loader": "^8.0.6",
    "bio-parsers": "^8.3.16",
    "bootstrap": "^4.5.0",
    "bootstrap-markdown-fa5": "2.10.*",
    "bootstrap-select": "^1.13.18",
    "css-loader": "^6.0.0",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "dropzone": "^5.8.0",
    "expose-loader": "^3.0.0",
    "file-saver": "2.0.*",
    "i18next": "^21.0.2",
    "jquery": "^3.5.1",
    "jquery-jeditable": "2.0.*",
    "jquery-ui": "^1.13.0",
    "jsoneditor": "^9.0.3",
    "luxon": "^2.0.1",
    "marked": "^4.0.0",
    "mathjax-full": "^3.2.0",
    "mini-css-extract-plugin": "^2.1.0",
    "popper.js": "1.16.*",
    "prismjs": "^1.25.0",
    "sass": "^1.23.7",
    "stream-browserify": "^3.0.0",
    "timers-browserify": "^2.0.12",
    "tinymce": "^5.10.0",
    "ts-loader": "^9.1.1",
    "typescript": "^4.0.3",
    "ve-range-utils": "^2.6.3",
    "ve-sequence-utils": "^5.1.6",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.0.17",
    "@types/fancybox": "^3.5.2",
    "@types/file-saver": "^2.0.1",
    "@types/jquery": "^3.5.1",
    "@types/jqueryui": "^1.12.14",
    "@types/luxon": "^2.0.0",
    "@types/tinymce": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "cypress": "^8.0.0",
    "eslint": "^6.8.0",
    "stylelint": "^14.0.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "webpack-bundle-analyzer": "^4.1.0"
  },
  "scripts": {
    "analyzejs": "webpack-bundle-analyzer stats.json web/app/js",
    "apidoc": "apidoc -c apidoc/apidoc.json -o apidoc/html -i src/controllers",
    "brotli": "brotli -vf web/assets/*.css web/assets/*.js web/assets/*.svg web/assets/ove/*.css web/assets/ove/*.js",
    "zopfli": "zopfli -i1000 web/assets/*.css web/assets/*.js web/assets/*.svg web/assets/ove/*.css web/assets/ove/*.js",
    "build": "webpack-cli --progress --config builder.js",
    "buildnode": "webpack-cli --progress --config node-builder.js",
    "buildall": "CYPRESS_INSTALL_BINARY=0 yarn install --pure-lockfile && yarn buildjs && yarn buildcss && yarn chemdoodle && yarn tinymce && yarn ove && yarn brotli && yarn zopfli",
    "buildcss": "sass --quiet-deps --style compressed src/scss/main.scss:web/assets/elabftw.min.css src/scss/pdf.scss:web/assets/pdf.min.css src/scss/cornify.scss:web/assets/cornify.min.css",
    "buildjs": "yarn build && yarn buildnode",
    "buildprofile": "webpack-cli --config builder.js --profile --json > stats.json",
    "buildnodeprofile": "webpack-cli --config node-builder.js --profile --json > node-stats.json",
    "chemdoodle": "cp node_modules/@deltablot/chemdoodle-web-mini/dist/chemdoodle.min.js web/assets",
    "clean": "rm -rf apidoc/html _srcdoc src/ts/*.js src/ts/*.js.map web/app/js/*bundle.js src/node/*bundle.js web/app/js/*.LICENSE.txt web/app/js/*.min.js web/app/css/*.min.css web/app/css/*.min.css.map web/assets/ove/*",
    "codespell": "codespell --config src/tools/codespellrc -L want to,sSee",
    "csslint": "./node_modules/.bin/stylelint src/scss/*.scss",
    "cy": "yarn run cypress run",
    "full": "yarn phpcs && yarn jslint && yarn csslint && yarn psalm && yarn phpstan && yarn test unit",
    "jslint": "eslint --ext .ts src/ts",
    "jslint-ci": "eslint --ext .ts --quiet src/ts",
    "ove": "mkdir -p web/assets/ove/ && cp node_modules/@deltablot/open-vector-editor-umd/* web/assets/ove/ && rm -f web/assets/ove/open-vector-editor.js*",
    "phan": "phan --allow-polyfill-parser -k src/tools/phan-config.php",
    "phpcs": "./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php -v --using-cache=no",
    "phpcs-dry": "./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php -v --dry-run --stop-on-violation --using-cache=no",
    "phpmetrics": "phpmetrics --report-html=metrics . && firefox metrics/index.html",
    "phpstan": "phpstan analyse --memory-limit 4G -c src/tools/phpstan.neon",
    "pre-commit": "yarn static && yarn phpcs-dry && yarn csslint && yarn jslint",
    "psalm": "psalm -c=src/tools/psalm.xml",
    "psalmall": "psalm -c=src/tools/psalm.xml --show-info=true",
    "rector": "docker run -v /home/ktr/.dev/elabftw:/project rector/rector:latest process /project/src --config /project/src/tools/rector.php --autoload-file /project/vendor/autoload.php",
    "rector-dry": "docker run -v /home/ktr/.dev/elabftw:/project rector/rector:latest process /project/src --config /project/src/tools/rector.php --autoload-file /project/vendor/autoload.php --dry-run",
    "srcdoc": "phpDocumentor run --cache-folder /tmp -i ./cache -i ./uploads -d src/classes -d src/maps -d src/controllers -d src/exceptions -d src/interfaces -d src/models -d src/services -d src/traits -d web/app/controllers -t _srcdoc",
    "static": "yarn psalm && yarn phpstan",
    "test": "tests/run.sh",
    "tinymce": "cp node_modules/tinymce/skins/ui/oxide/fonts/tinymce-mobile.woff web/app/css/tinymce/fonts/tinymce-mobile.woff && cp node_modules/tinymce/skins/ui/oxide/*.min.css web/app/css/tinymce/",
    "unit": "tests/run.sh unit",
    "unit-ci": "php vendor/bin/codecept run unit",
    "watchcss": "sass --quiet-deps --watch src/scss/main.scss:web/assets/elabftw.min.css",
    "watchjs": "webpack-cli --config builder.js --mode=development --watch --progress",
    "watchnode": "webpack-cli --config node-builder.js --mode=development --watch --progress"
  },
  "repository": "git+https://github.com/elabftw/elabftw.git",
  "author": "Nicolas CARPi",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/elabftw/elabftw/issues"
  },
  "homepage": "https://www.elabftw.net"
}
