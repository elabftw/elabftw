{
    "name": "elabftw/elabftw",
    "description": "Laboratory notebook for research labs",
    "homepage": "https://www.elabftw.net",
    "require": {
        "php": ">=5.6.0",
        "ext-ctype": "*",
        "ext-curl":"*",
        "ext-gettext":"*",
        "ext-openssl":"*",
        "ext-gmagick":"*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "defuse/php-encryption": "2.0.*",
