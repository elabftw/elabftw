{
  "name": "elabftw/elabftw",
  "description": "Laboratory notebook for research labs",
  "homepage": "https://www.elabftw.net",
  "require": {
    "php": ">=8.0.1",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-exif": "*",
    "ext-fileinfo": "*",
    "ext-filter": "*",
    "ext-gettext": "*",
    "ext-hash": "*",
    "ext-json": "*",
    "ext-ldap": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-gd": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "defuse/php-encryption": "2.3.*",
    "mpdf/mpdf": "8.0.*",
    "onelogin/php-saml": "^4.0.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "symfony/http-foundation": "^6.0",
    "twig/twig": "^3.3.8",
    "monolog/monolog": "^2.1.0",
    "maennchen/zipstream-php": "^2.0.0",
    "league/csv": "^9.4",
    "mpdf/qrcode": "^1.0",
    "symfony/console": "^5.0",
    "symfony/process": "^6.0",
    "league/commonmark": "^2.0",
    "symfony/filesystem": "^6.0",
    "symfony/mime": "^6.0",
    "robthree/twofactorauth": "^1.7.0",
    "directorytree/ldaprecord": "^2.0.3",
    "league/flysystem": "^3.0",
    "league/flysystem-memory": "^3.0",
    "lcobucci/jwt": "^4.1",
    "ezyang/htmlpurifier": "^4.13",
    "symfony/mailer": "^6.0",
    "xemlock/htmlpurifier-html5": "^0.1.11",
    "aws/aws-sdk-php": "^3.209",
    "league/flysystem-aws-s3-v3": "^3.0",
    "jblond/twig-trans": "^1.0",
    "twig/intl-extra": "^3.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "codeception/codeception": "^4.1.24",
    "pyrech/composer-changelogs": "^1.8.0",
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "codeception/module-webdriver": "^1.0.0",
    "codeception/module-db": "^1.0.0",
    "codeception/module-asserts": "^1.0.0",
    "codeception/module-rest": "^1.0",
    "codeception/module-phpbrowser": "^1.0",
    "symfony/yaml": "^5.0",
    "fakerphp/faker": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "Elabftw\\Controllers\\": [
        "src/controllers"
      ],
      "Elabftw\\Elabftw\\": [
        "src/classes"
      ],
      "Elabftw\\Commands\\": [
        "src/commands"
      ],
      "Elabftw\\Exceptions\\": [
        "src/exceptions"
      ],
      "Elabftw\\Interfaces\\": [
        "src/interfaces"
      ],
      "Elabftw\\Maps\\": [
        "src/maps"
      ],
      "Elabftw\\Models\\": [
        "src/models"
      ],
      "Elabftw\\Services\\": [
        "src/services"
      ],
      "Elabftw\\Services\\AdvancedSearchQuery\\Collectors\\": [
        "src/services/advancedSearchQuery/collectors"
      ],
      "Elabftw\\Services\\AdvancedSearchQuery\\Exceptions\\": [
        "src/services/advancedSearchQuery/exceptions"
      ],
      "Elabftw\\Services\\AdvancedSearchQuery\\Grammar\\": [
        "src/services/advancedSearchQuery/grammar",
        "cache/advancedSearchQuery"
      ],
      "Elabftw\\Services\\AdvancedSearchQuery\\Interfaces\\": [
        "src/services/advancedSearchQuery/interfaces"
      ],
      "Elabftw\\Services\\AdvancedSearchQuery\\Visitors\\": [
        "src/services/advancedSearchQuery/visitors"
      ],
      "Elabftw\\Traits\\": [
        "src/traits"
      ]
    }
  },
  "license": "AGPL-3.0-or-later",
  "config": {
    "allow-plugins": {
      "pyrech/composer-changelogs": true
    }
  }
}
