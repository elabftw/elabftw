{
  "name": "elabftw/elabftw",
  "description": "Laboratory notebook for research labs",
  "homepage": "https://www.elabftw.net",
  "require": {
    "php": ">=7.1.3",
    "ext-ctype": "*",
    "ext-curl":"*",
    "ext-gettext":"*",
    "ext-openssl":"*",
    "ext-gd":"*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "defuse/php-encryption": "2.2.*",
    "mpdf/mpdf": "8.0.*",
    "swiftmailer/swiftmailer":"6.2.*",
    "onelogin/php-saml": "3.1.*",
    "guzzlehttp/guzzle": "6.3.*",
    "michelf/php-markdown": "1.8.*",
    "symfony/http-foundation": "4.2.*",
    "twig/twig": "2.9.*",
    "twig/extensions": "1.5.*",
    "monolog/monolog": "1.24.*",
    "maennchen/zipstream-php": "^1.0.0",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "league/csv": "^9.2",
    "mpdf/qrcode": "^1.0",
    "symfony/console": "^4.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "codeception/codeception": "3.0.*",
    "pyrech/composer-changelogs": "1.6.*",
    "fzaninotto/faker": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Elabftw\\Controllers\\" : ["src/controllers"],
      "Elabftw\\Elabftw\\" : ["src/classes"],
      "Elabftw\\Commands\\" : ["src/commands"],
      "Elabftw\\Exceptions\\" : ["src/exceptions"],
      "Elabftw\\Interfaces\\" : ["src/interfaces"],
      "Elabftw\\Models\\" : ["src/models"],
      "Elabftw\\Services\\" : ["src/services"],
      "Elabftw\\Traits\\" : ["src/traits"]
    }
  },
  "license": "AGPL-3.0-or-later"
}
