{
  "name": "elabftw",
  "description": "Free and open source electronic lab notebook for research labs",
  "main": "Gruntfile.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "3dmol": "^1.4.0",
    "@fancyapps/fancybox": "3.5.*",
    "@fortawesome/fontawesome-svg-core": "1.2.*",
    "@fortawesome/free-brands-svg-icons": "5.12.*",
    "@fortawesome/free-regular-svg-icons": "5.12.*",
    "@fortawesome/free-solid-svg-icons": "5.12.*",
    "@fullcalendar/bootstrap": "^4.3.0",
    "@fullcalendar/core": "^4.3.0",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/list": "^4.3.0",
    "@fullcalendar/timegrid": "^4.3.0",
    "bootstrap": "~4.4.*",
    "bootstrap-markdown-fa5": "2.10.*",
    "dropzone": "5.7.*",
    "file-saver": "2.0.*",
    "jquery": "3.4.*",
    "jquery-jeditable": "2.0.*",
    "jquery-ui": "^1.12.1",
    "jsoneditor": "^8.6.1",
    "marked": "0.8.*",
    "mathjax": "3.0.*",
    "moment": "2.24.*",
    "popper.js": "1.16.*",
    "prismjs": "1.19.*",
    "sass": "^1.23.7",
    "tinymce": "5.2.*"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/bootstrap": "^4.3.1",
    "@types/fancybox": "^3.5.2",
    "@types/file-saver": "^2.0.1",
    "@types/jquery": "^3.3.31",
    "@types/jqueryui": "^1.12.9",
    "@types/tinymce": "^4.5.23",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "apidoc": "0.20.*",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "expose-loader": "0.7.*",
    "google-charts": "^2.0.0",
    "grunt": "1.0.*",
    "grunt-contrib-cssmin": "3.0.*",
    "grunt-contrib-uglify-es": "gruntjs/grunt-contrib-uglify#harmony",
    "grunt-contrib-watch": "1.1.*",
    "grunt-shell": "3.0.*",
    "html-webpack-plugin": "^3.2.0",
    "lodash": ">=4.17.13",
    "snyk": "1.*",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "webpack": "^4.39.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "3.3.*"
  },
  "scripts": {
    "analyzejs": "webpack-bundle-analyzer stats.json web/app/js",
    "apidoc": "apidoc -o _apidoc -i src/controllers",
    "build": "webpack-cli --config builder.js",
    "buildall": "grunt && yarn build && sass --style compressed src/scss/main.scss:web/app/css/elabftw.min.css",
    "buildprofile": "webpack-cli --config builder.js --profile --json > stats.json",
    "clean": "rm -rf _apidoc _srcdoc src/ts/*.js src/ts/*.js.map web/app/js/*bundle.js web/app/js/*.min.js web/app/css/*.min.css",
    "coverage": "php vendor/bin/codecept run --skip acceptance --coverage --coverage-html",
    "jslint": "eslint --ext .ts src/ts",
    "jslint-ci": "eslint --ext .ts --quiet src/ts",
    "phan": "phan --allow-polyfill-parser -k src/tools/phan-config.php",
    "phpcs": "./vendor/bin/php-cs-fixer fix --config=.php_cs.dist -v --using-cache=no",
    "phpmetrics": "phpmetrics --report-html=metrics . && firefox metrics/index.html",
    "phpstan": "phpstan analyse --memory-limit 2G -c src/tools/phpstan.neon",
    "psalm": "psalm -c=src/tools/psalm.xml",
    "srcdoc": "phpDocumentor run --cache-folder /tmp -i ./cache -i ./uploads -d src/classes -d src/controllers -d src/exceptions -d src/interfaces -d src/models -d src/services -d src/traits -d web/app/controllers -t _srcdoc",
    "test": "tests/run.sh",
    "unit": "php vendor/bin/codecept run unit",
    "vulns": "snyk test",
    "watchcss": "sass --watch src/scss/main.scss:web/app/css/elabftw.min.css",
    "watchjs": "grunt && webpack-cli --config builder.js --watch"
  },
  "repository": "git+https://github.com/elabftw/elabftw.git",
  "author": "Nicolas CARPi",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/elabftw/elabftw/issues"
  },
  "homepage": "https://www.elabftw.net"
}
