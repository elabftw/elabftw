{
  "name": "elabftw/elabftw",
  "description": "Laboratory notebook for research labs",
  "homepage": "https://www.elabftw.net",
  "require": {
    "php": ">=7.3.7",
    "ext-ctype": "*",
    "ext-curl":"*",
    "ext-exif": "*",
    "ext-fileinfo": "*",
    "ext-filter": "*",
    "ext-gettext":"*",
    "ext-hash": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl":"*",
    "ext-gd":"*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "defuse/php-encryption": "2.2.*",
    "mpdf/mpdf": "8.0.*",
    "swiftmailer/swiftmailer":"6.2.*",
    "onelogin/php-saml": "3.3.*",
    "guzzlehttp/guzzle": "6.4.*",
    "symfony/http-foundation": "4.3.*",
    "twig/twig": "2.12.*",
    "twig/extensions": "1.5.*",
    "monolog/monolog": "2.0.*",
    "maennchen/zipstream-php": "^1.0.0",
    "league/csv": "^9.4",
    "mpdf/qrcode": "^1.0",
    "symfony/console": "^4.3",
    "symfony/process": "^4.3",
    "league/commonmark": "^1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "codeception/codeception": "3.1.*",
    "pyrech/composer-changelogs": "1.7.*",
    "fzaninotto/faker": "^1.9",
    "friendsofphp/php-cs-fixer": "^2.15"
  },
  "autoload": {
    "psr-4": {
      "Elabftw\\Controllers\\" : ["src/controllers"],
      "Elabftw\\Elabftw\\" : ["src/classes"],
      "Elabftw\\Commands\\" : ["src/commands"],
      "Elabftw\\Exceptions\\" : ["src/exceptions"],
      "Elabftw\\Interfaces\\" : ["src/interfaces"],
      "Elabftw\\Models\\" : ["src/models"],
      "Elabftw\\Services\\" : ["src/services"],
      "Elabftw\\Traits\\" : ["src/traits"]
    }
  },
  "license": "AGPL-3.0-or-later"
}
