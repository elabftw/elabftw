{
    "name": "elabftw/elabftw",
    "description": "Laboratory notebook for research labs",
    "homepage": "https://www.elabftw.net",
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-curl":"*",
        "ext-gettext":"*",
        "ext-openssl":"*",
        "ext-gd":"*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "defuse/php-encryption": "2.2.*",
        "mpdf/mpdf": "7.1.*",
        "swiftmailer/swiftmailer":"6.1.*",
        "onelogin/php-saml": "3.0.*",
        "guzzlehttp/guzzle": "6.3.*",
        "michelf/php-markdown": "1.8.*",
        "symfony/http-foundation": "4.1.*",
        "twig/twig": "2.5.*",
        "twig/extensions": "1.5.*",
        "monolog/monolog": "1.23.*",
        "maennchen/zipstream-php": "dev-master"

    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "codeception/codeception": "2.5.*",
        "pyrech/composer-changelogs": "1.6.*"
    },
    "autoload": {
        "psr-4": {
            "Elabftw\\Elabftw\\" : ["src/classes", "src/models", "src/views"]
        }
    },
    "license": "AGPL-3.0-or-later"
}
